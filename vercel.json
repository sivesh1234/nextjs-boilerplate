{
    "devCommand": "uvicorn main:app --host 0.0.0.0 --port 3000",
    "framework": "nextjs",
    "builds": [
      {
        "src": "FastAPI_Server/main.py",
        "use": "@vercel/python"
      },
        {
            "src": "app/page.tsx",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "FastAPI_Server/main.py"
        },
        {
            "src": "/(.*)",
            "dest": "app/page.tsx"
        }
    ]
  }